{
  "name": "spread-eagle",
  "version": "0.1.0",
  "dependencies": {
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.6.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.3",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "run-p fix:*",
    "fix:css": "run-s fix:css:prettier",
    "fix:css:prettier": "prettier './**/*.css' --write",
    "fix:js": "run-s fix:js:eslint fix:js:prettier",
    "fix:js:eslint": "next lint --fix",
    "fix:js:prettier": "prettier './**/*.{js,jsx,ts,tsx}' --write",
    "fix:json": "prettier './**/*.json' --write",
    "fix:md": "prettier './**/*.md' --write",
    "lint": "run-p lint:*",
    "lint:css": "run-p lint:css:*",
    "lint:css:prettier": "prettier './**/*.css' --check",
    "lint:js": "run-p lint:js:*",
    "lint:js:eslint": "next lint",
    "lint:js:prettier": "prettier './**/*.{js,jsx,ts,tsx}' --check",
    "lint:js:tsc": "tsc --noEmit",
    "lint:json": "prettier './**/*.json' --check",
    "lint:md": "prettier './**/*.md' --check",
    "start": "next start"
  }
}
